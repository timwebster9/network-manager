<!DOCTYPE html>
<html lang="en">
<head>
	<% include ../partials/head %>
</head>
<body class="container">

	<header>
		<% include ../partials/header %>
	</header>

	<main>
		<div class="jumbotron">
			<h1>Network Manager</h1>
                        <h2>Current Status</h2>
                        <ul>
                          <% machines.forEach(function(machine) { %>
                            <li><%= machine.name %> - <%= machine.status %></li>
                          <% }); %>
                        </ul>
		</div>
	</main>

	<footer>
		<% include ../partials/footer %>
	</footer>
	
</body>
</html>
